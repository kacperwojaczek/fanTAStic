Create table Tags
(name varchar(30) not null primary key,
count int,
);

Create table Wall_LoginUsera
(id int not null primary key,  
tytul varchar(75),
autor int default 0,
tresc text,
tagi binary(500),
attachment varchar(150)
);

Create table Users
(id int not null primary key identity(1,1),
imiebloggera varchar(64),
nazwiskobloggera varchar(80),
nazwabloga varchar(256),
Login nvarchar(128) unique,
Password nvarchar(128)
);
